---
import Layout from '../layouts/LayoutWorkbench.astro';
---
<Layout title="Workbench | Menu">
    <div class="main-content">
        <div class="page-content">
            <div class="container-fluid">
                <!-- start page title -->
                <div class="row">
                    <div class="col-12">
                        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                            <h2 class="mb-sm-0 font-size-23">Workbench Menu </h2>
                        </div>
                    </div>
                </div>
                <!-- end page title -->
                
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card job-filter">
                            <div class="card-body p-3">
                                <form action="javascript:void(0);">
                                    <div class="row g-3">
                                        <div class="col-xxl-5 col-lg-6">
                                            <div class="position-relative">
                                                <input type="text" class="form-control" id="searchList" autocomplete="off" placeholder="Search Workbench">
                                            </div>
                                        </div>
                                        <!--end col-->             
                                        <div class="col-xxl-2 col-lg-2">
                                            <div class="position- h-100 hstack gap-1 d-flex justify-content-between">
                                                <!-- <button type="button" class="btn btn-primary waves-effect btn-label waves-light"onclick="anotherFunction();"><i class="bx bx-search label-icon"></i> Search</button> -->
                                                <a href="/create-workbench" type="button" class="btn btn-success waves-effect btn-label waves-light"onclick="anotherFunction();"><i class="bx bx-plus label-icon"></i> add </button></a>
                                            </div>
                                        </div> 
                                        <!--end col-->
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
                    <!--end col-->
                </div>
                <!--end row-->
                <div class="row" id="candidate-list">
                    <div class="col-xl-3 col-md-6">
                        <div class="card">
                            <div class="card-body">
                                <div class="d-flex align-start mb-3">
                                    <div class="flex-grow-1">
                                        <h5><span class="badge badge-soft-danger">Malware</span></h5>
                                    </div>
                                </div>
                                <div class="text-center mb-3">
                                   <img src="assets/images/users/avatar-10.png" alt="" style="width: 300px; height: 200px; border-radius: 0;" />
                                    <h6 class="font-size-15 mt-3 mb-1">SMB malware</h6>
                                    <p class="mb-0 text-muted">Virus Found in File </p>
                                </div>
                                <div class="d-flex mb-3 justify-content-center gap-2 text-muted">
                                    <div>
                                        <i class='bx bx-map align-middle text-primary'></i> False Positive 
                                    </div>
                                </div>
                                <div class="mt-4 pt-1">
                                    <a href="/analisis" class="btn btn-soft-primary d-block">View analisis</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3">
                        <div class="card">
                            <div class="card-body">
                                <div class="d-flex align-start mb-3">
